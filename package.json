{
  "name": "latex-non-academic-word-check",
  "displayName": "latex-non-academic-word-check",
  "publisher": "Jun",
  "repository": {
    "url": "https://github.com/youyinnn/latex-non-academic-word-check",
    "type": "extension"
  },
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:latex",
    "onFileSystem:file"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.checkAllTexFiles",
        "title": "Latex NAWC: Check All .tex Files in Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "comment-json": "^4.2.5"
  }
}
